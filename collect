#!/bin/bash
# Author: Shree Raj Shrestha
# Date: 02/09/2025
set -ueox pipefail

source "./home"
source "${HUSK_HOME}/rake" "${1}" "${2}"
if [[ "${RAKE_PATH}" =~ .*cob$ ]]; then ./gather "${WHICH_SILO}" "${WHICH_SOIL}"; fi
./spin "${WHICH_SILO}" "${WHICH_SOIL}"
./incubate "${WHICH_SILO}" "${WHICH_SOIL}"
./peek "${WHICH_SILO}" "${WHICH_SOIL}"
