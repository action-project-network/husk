STEPS=(
    "./configure --prefix=${SOIL_HOME}/${WHICH_COB}/${WHICH_SEED}"
    "make -j12"
    "make testprog"
    "./testprog > testprog.lis"
    "if ! diff -q testprog.lis testprog.out &>/dev/null; then >&2 exit 1; fi"
    "if ! cmp testprog.fit testprog.std &>/dev/null; then >&2 exit 1; fi"
    "make install"
)