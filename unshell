#!/bin/sh
# Author: Shree Raj Shrestha
# Date: 01/30/2025
set -ueox pipefail
source /soil/rake "${1}"
source "${SOIL_HOME}/silo/cobs/${COB_PATH}.cob"
for SEED in "${COB[@]}"; do
    if [ ! -d "${SOIL_HOME}/${WHICH_COB}/$(echo "${SEED}" | cut -d "_" -f 1)" ]; then
        "${SOIL_HOME}/sprout" "${WHICH_POD}" "${SEED}";
    fi
done
